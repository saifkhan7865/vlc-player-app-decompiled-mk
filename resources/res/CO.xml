<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:aapt="http://schemas.android.com/aapt">
    <item android:state_focused="true">
        <set>
            <objectAnimator android:duration="200" android:valueTo="12dp" android:valueType="floatType" android:propertyName="elevation"/>
        </set>
    </item>
    <item>
        <set>
            <objectAnimator android:duration="200" android:valueTo="1dp" android:valueType="floatType" android:propertyName="elevation"/>
        </set>
    </item>
</selector>
